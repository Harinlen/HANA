{
  "files": {
    "contigs": "contigs_sim.fasta",
    "reads": [
    	["SRR6470741_1.fastq.gz", "SRR6470741_2.fastq.gz"]
    ],
    "bams": []
  },
  "global": {
    "groups": 12,
    "threads": 8,
    "enzyme": "MboI"
  },
  "ops": [
    {
      "command": "map",
      "enable": false,
      "mapper": "bwa",
      "contigs": "$contigs",
      "reads": "$reads",
      "args": []
    },
    {
      "command": "extract"
    },
    {
      "command": "draft"
    },
    {
      "command": "partition"
    },
    {
      "command": "loop",
      "var": "group",
      "list": "-groups",
      "body": [
        {
          "command": "ordering",
          "enable": true
        }
      ]
    },
    {
      "command": "orientation"
    },
    {
      "command": "build"
    }
  ]
}
